{
  "name": "@trpc/client",
  "version": "9.24.0",
  "description": "tRPC Client lib",
  "author": "KATT",
  "license": "MIT",
  "homepage": "https://trpc.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trpc/trpc.git",
    "directory": "packages/client"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "links/httpLink.ts",
      "links/httpBatchLink.ts",
      "links/splitLink.ts",
      "links/loggerLink.ts",
      "links/wsLink.ts"
    ]
  },
  "scripts": {},
  "main": "dist/trpc-client.cjs.js",
  "module": "dist/trpc-client.esm.js",
  "typings": "dist/trpc-client.cjs.d.ts",
  "exports": {
    ".": {
      "import": "./dist/trpc-client.esm.js",
      "require": "./dist/trpc-client.cjs.js",
      "types": "./dist/trpc-client.cjs.d.ts"
    },
    "./links/httpBatchLink": {
      "import": "./links/dist/trpc-client-links-httpBatchLink.esm.js",
      "require": "./links/dist/trpc-client-links-httpBatchLink.cjs.js",
      "types": "./links/dist/trpc-client-links-httpBatchLink.cjs.d.js"
    },
    "./links/httpLink": {
      "import": "./links/dist/trpc-client-links-httpLink.esm.js",
      "require": "./links/dist/trpc-client-links-httpLink.cjs.js",
      "types": "./links/dist/trpc-client-links-httpLink.cjs.d.js"
    },
    "./links/loggerLink": {
      "import": "./links/dist/trpc-client-links-loggerLink.esm.js",
      "require": "./links/dist/trpc-client-links-loggerLink.cjs.js",
      "types": "./links/dist/trpc-client-links-loggerLink.cjs.d.js"
    },
    "./links/splitLink": {
      "import": "./links/dist/trpc-client-links-splitLink.esm.js",
      "require": "./links/dist/trpc-client-links-splitLink.cjs.js",
      "types": "./links/dist/trpc-client-links-splitLink.cjs.d.js"
    },
    "./links/wsLink": {
      "import": "./links/dist/trpc-client-links-wsLink.esm.js",
      "require": "./links/dist/trpc-client-links-wsLink.cjs.js",
      "types": "./links/dist/trpc-client-links-wsLink.cjs.d.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "links",
    "src"
  ],
  "dependencies": {
    "@babel/runtime": "^7.9.0"
  },
  "peerDependencies": {
    "@trpc/server": "^9.12.0"
  },
  "devDependencies": {
    "@trpc/server": "^9.24.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
