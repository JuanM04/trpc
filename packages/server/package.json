{
  "name": "@trpc/server",
  "version": "9.24.0",
  "description": "tRPC Server",
  "author": "KATT",
  "license": "MIT",
  "homepage": "https://trpc.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trpc/trpc.git",
    "directory": "packages/server"
  },
  "eslintConfig": {
    "rules": {
      "explicit-module-boundary-types": "off"
    }
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "adapters/express.ts",
      "adapters/fastify/index.ts",
      "adapters/next.ts",
      "adapters/node-http/index.ts",
      "adapters/standalone.ts",
      "adapters/ws.ts",
      "ws/index.ts",
      "rpc/index.ts"
    ]
  },
  "scripts": {
    "test": "jest"
  },
  "main": "dist/trpc-server.cjs.js",
  "module": "dist/trpc-server.esm.js",
  "typings": "dist/trpc-server.cjs.d.ts",
  "exports": {
    ".": {
      "import": "./dist/trpc-server.esm.js",
      "require": "./dist/trpc-server.cjs.js",
      "types": "./dist/trpc-server.cjs.d.ts"
    },
    "./adapters/express": {
      "import": "./adapters/express/dist/trpc-server-adapters-express.esm.js",
      "require": "./adapters/express/dist/trpc-server-adapters-express.cjs.js",
      "types": "./adapters/express/dist/trpc-server-adapters-express.cjs.d.js"
    },
    "./adapters/fastify": {
      "import": "./adapters/fastify/dist/trpc-server-adapters-fastify.esm.js",
      "require": "./adapters/fastify/dist/trpc-server-adapters-fastify.cjs.js",
      "types": "./adapters/fastify/dist/trpc-server-adapters-fastify.cjs.d.js"
    },
    "./adapters/next": {
      "import": "./adapters/next/dist/trpc-server-adapters-next.esm.js",
      "require": "./adapters/next/dist/trpc-server-adapters-next.cjs.js",
      "types": "./adapters/next/dist/trpc-server-adapters-next.cjs.d.js"
    },
    "./adapters/node-http": {
      "import": "./adapters/node-http/dist/trpc-server-adapters-node-http.esm.js",
      "require": "./adapters/node-http/dist/trpc-server-adapters-node-http.cjs.js",
      "types": "./adapters/node-http/dist/trpc-server-adapters-node-http.cjs.d.js"
    },
    "./adapters/standalone": {
      "import": "./adapters/standalone/dist/trpc-server-adapters-standalone.esm.js",
      "require": "./adapters/standalone/dist/trpc-server-adapters-standalone.cjs.js",
      "types": "./adapters/standalone/dist/trpc-server-adapters-standalone.cjs.d.js"
    },
    "./adapters/ws": {
      "import": "./adapters/ws/dist/trpc-server-adapters-ws.esm.js",
      "require": "./adapters/ws/dist/trpc-server-adapters-ws.cjs.js",
      "types": "./adapters/ws/dist/trpc-server-adapters-ws.cjs.d.js"
    },
    "./rpc": {
      "import": "./rpc/dist/trpc-server-rpc.esm.js",
      "require": "./rpc/dist/trpc-server-rpc.cjs.js",
      "types": "./rpc/dist/trpc-server-rpc.cjs.d.js"
    },
    "./ws": {
      "import": "./ws/dist/trpc-server-ws.esm.js",
      "require": "./ws/dist/trpc-server-ws.cjs.js",
      "types": "./ws/dist/trpc-server-ws.cjs.d.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "adapters",
    "ws",
    "rpc",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@fastify/websocket": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/hash-sum": "^1.0.0",
    "@types/ws": "^8.2.0",
    "abort-controller": "^3.0.0",
    "devalue": "^2.0.1",
    "express": "^4.17.1",
    "fastify": "^3.27.1",
    "fastify-plugin": "^3.0.1",
    "hash-sum": "^2.0.0",
    "jest": "^27.1.0",
    "konn": "^0.7.0",
    "myzod": "^1.3.1",
    "next": "^12.1.6",
    "node-fetch": "^2.6.1",
    "superstruct": "^0.15.2",
    "typescript": "4.4.4",
    "ws": "^8.0.0",
    "yup": "^0.32.8",
    "zod": "^3.0.0"
  }
}
